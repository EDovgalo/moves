{
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": "error"
    },
    "lines-between-class-members": [
        "error",
        "always",
        { "exceptAfterSingleLine": false }
    ]
}
